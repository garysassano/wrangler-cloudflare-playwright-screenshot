{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "wrangler-cloudflare-playwright",
	"main": "src/index.ts",
	"workers_dev": true,
	"compatibility_flags": ["nodejs_compat_v2"],
	"compatibility_date": "2025-06-30",
	"upload_source_maps": true,
	"browser": {
	  "binding": "MYBROWSER"
	},
